{
  "name": "@capitalis/nuxt-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": "22",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:vercel": "NITRO_PRESET=vercel nuxt build --no-typecheck",
    "build:vercel-prod": "NODE_ENV=production nuxt build --config nuxt.config.vercel.ts",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier --write .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@nuxt/ui": "4.0.0-alpha.1",
    "@nuxtjs/color-mode": "^3.3.1",
    "@nuxtjs/hanko": "^1.0.0",
    "@nuxtjs/i18n": "^10.0.6",
    "@nuxtjs/robots": "^5.5.1",
    "@pinia/nuxt": "^0.11.2",
    "@teamhanko/hanko-elements": "^2.1.1",
    "@teamhanko/hanko-frontend-sdk": "^2.1.1",
    "@vee-validate/zod": "^4.15.1",
    "@vue/apollo-composable": "^4.0.0-rc.13",
    "nuxt": "^4.0.3",
    "nuxt-og-image": "^5.1.9",
    "nuxt-schema-org": "^5.0.6",
    "vue": "^3.5.21",
    "vue-router": "^4.2.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/eslint-plugin": "^1.4.1",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}